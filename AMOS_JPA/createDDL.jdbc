CREATE TABLE "Kundendaten" ("ID"  SERIAL NOT NULL, "Geburtstag" DATE, "Vorname" VARCHAR(255), "Gender" VARCHAR(255), "Mail" VARCHAR(255), "Nachname" VARCHAR(255), "Wohnort" VARCHAR(255), "ZipCode" VARCHAR(255), PRIMARY KEY ("ID"))
CREATE TABLE "Userdatas" ("ID"  SERIAL NOT NULL, "f_token" VARCHAR(250), "f_token_secret" VARCHAR(250), "l_token" VARCHAR(250), "l_token_secret" VARCHAR(250), "t_token" VARCHAR(250), "t_token_secret" VARCHAR(250), "usermail" VARCHAR(50), "username" VARCHAR(50), "userpass" VARCHAR(250), "x_token" VARCHAR(250), "x_token_secret" VARCHAR(250), PRIMARY KEY ("ID"))
CREATE TABLE "TwitterDatas" ("ID"  SERIAL NOT NULL, "data" VARCHAR(1000), "type" VARCHAR(25), ClientID INTEGER, PRIMARY KEY ("ID"))
CREATE TABLE "FacebookDatas" ("ID"  SERIAL NOT NULL, "data" VARCHAR(1000), "type" VARCHAR(25), ClientID INTEGER, PRIMARY KEY ("ID"))
ALTER TABLE "TwitterDatas" ADD CONSTRAINT "FK_TwitterDatas_ClientID" FOREIGN KEY (ClientID) REFERENCES "Kundendaten" (ID)
ALTER TABLE "FacebookDatas" ADD CONSTRAINT "FK_FacebookDatas_ClientID" FOREIGN KEY (ClientID) REFERENCES "Kundendaten" (ID)
