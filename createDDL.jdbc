CREATE TABLE "Kundendaten" ("ID"  SERIAL NOT NULL, "Geburtstag" DATE, "Vorname" VARCHAR(255), "Gender" VARCHAR(255), "Mail" VARCHAR(255), "Nachname" VARCHAR(255), "Wohnort" VARCHAR(255), "ZipCode" VARCHAR(255), PRIMARY KEY ("ID"))
CREATE TABLE "Userdatas" ("ID"  SERIAL NOT NULL, "f_username" VARCHAR(50), "f_userpass" VARCHAR(50), "l_username" VARCHAR(50), "l_userpass" VARCHAR(50), "t_username" VARCHAR(50), "t_userpass" VARCHAR(50), "usermail" VARCHAR(50), "username" VARCHAR(50), "userpass" VARCHAR(250), "x_username" VARCHAR(50), "x_userpass" VARCHAR(50), PRIMARY KEY ("ID"))
